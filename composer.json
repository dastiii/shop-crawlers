{
    "name": "dastiii/shop-crawlers",
    "description": "",
    "keywords": [
        "dastiii",
        "crawlers"
    ],
    "homepage": "https://github.com/dastiii/shop-crawlers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tobias Schwarz",
            "email": "dastiii@schwarz.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "fabpot/goutte": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^v6.17.0",
        "pestphp/pest": "^1.2",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Dastiii\\Crawlers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dastiii\\Crawlers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
